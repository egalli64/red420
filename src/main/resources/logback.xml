<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss:SSS} %-5level [%thread] %logger{36}.%M %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="Servlet" level="trace" />

    <root level="debug">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>